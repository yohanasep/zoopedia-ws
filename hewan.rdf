      <animal:taxon>Araneae</animal:taxon>
      <animal:phylum>Arthropoda</animal:phylum>
      <animal:class>Arachnida</animal:class>
      <animal:order>Araneae</animal:order>
      <animal:family>Caponiidae, Theridiosomatidae, Deinopidae</animal:family>
      <animal:genus>Mastophora, Portia, Anelosimus</animal:genus>
    </animal:Description>

    <animal:Description rdf:about="http://dbpedia.org/resource/Crab">
